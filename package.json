{
  "name": "hola_mundo",
  "version": "1.0.0",
  "description": "primera implementacion de api graphql",
  "main": "build/server.js",
  "scripts": {
    "start": "node build/server.js",
    "start:dev": "npm run build:dev",
    "build": "tsc -p . && ncp src/schema build/schema",
    "build:dev": "nodemon 'src/server.ts' --exec 'ts-node' src/server.ts -e ts,graphql"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nicolas-Pistillo/graph_api.git"
  },
  "keywords": [
    "graphql",
    "api"
  ],
  "author": "Nicolas Pistillo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Nicolas-Pistillo/graph_api/issues"
  },
  "homepage": "https://github.com/Nicolas-Pistillo/graph_api#readme",
  "dependencies": {
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.2",
    "graphql-import-node": "0.0.4",
    "graphql-playground-middleware-express": "^1.7.22",
    "graphql-tools": "^8.1.0",
    "http": "0.0.1-security",
    "ncp": "^2.0.0",
    "typescript": "^4.4.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-graphql": "^0.9.0",
    "@types/graphql": "^14.5.0",
    "@types/node": "^16.7.8",
    "nodemon": "^2.0.12"
  }
}
